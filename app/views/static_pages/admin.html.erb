<%= provide(:title, 'Admin Sign In') %>
<h1>Admin Sign In</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(:static_pages, url: signin_path) do |f| %>

      <%= f.label :username %>
      <%= f.text_field :name %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
    <% end %>

  </div>
</div>


